  "scripts": {
    "build": "cross-env NODE_ENV=production pnpm exec nx run knowii:build",
    "clean": "pnpm exec rimraf ./dist",
    "clean:install": "pnpm exec rimraf pnpm-lock.yaml && pnpm exec rimraf ./node_modules && pnpm exec rimraf dist && pnpm install",
    "db:admin": "docker run --rm -p 5050:5050 thajeztah/pgadmin4",
    "db:clean": "pnpm exec rimraf ./supabase && pnpm exec supabase init && sh ./tools/supabase-set-local-ports.sh",
    "db:client:generate": "pnpm exec prisma generate && pnpm run format",
    "db:init": "pnpm run db:migration:deploy && pnpm run db:seed",
    "db:migration:deploy": "dotenv -e apps/knowii/.env.local -- pnpm exec prisma migrate deploy --preview-feature",
    "db:migration:deploy:prod": "dotenv -e apps/knowii/.env.prod -- pnpm exec prisma migrate deploy --preview-feature",
    "db:migration:prepare": "dotenv -e apps/knowii/.env.local -- pnpm exec prisma migrate dev --preview-feature",
    "db:migration:status": "dotenv -e apps/knowii/.env.local -- pnpm exec prisma migrate status --preview-feature",
    "db:migration:status:prod": "dotenv -e apps/knowii/.env.prod -- pnpm exec prisma migrate status --preview-feature",
    "db:reset": "pnpm run db:stop && pnpm run db:clean && pnpm run db:start && pnpm run db:init && pnpm run db:types:generate",
    "db:schema:introspect": "dotenv -e apps/knowii/.env.local -- pnpm exec prisma introspect",
    "db:schema:introspect:prod": "dotenv -e apps/knowii/.env.prod -- pnpm exec prisma introspect",
    "db:schema:validate": "dotenv -e apps/knowii/.env.local -- pnpm exec prisma validate",
    "db:schema:validate:prod": "dotenv -e apps/knowii/.env.prod -- pnpm exec prisma validate",
    "db:setup": "pnpm run db:clean && pnpm db:client:generate",
    "db:pull": "dotenv -e apps/knowii/.env.local -- pnpm exec prisma db pull",
    "predb:seed": "psql postgresql://postgres:postgres@localhost:12346/postgres -f supabase-db-seed.sql",
    "db:seed": "dotenv -e apps/knowii/.env.local -- pnpm exec prisma db seed",
    "db:start": "pnpm exec supabase start",
    "db:stop": "pnpm exec supabase stop",
    "db:studio:prisma": "dotenv -e apps/knowii/.env.local -- pnpm exec prisma studio",
    "db:studio": "open http://localhost:12347",
    "db:types:generate": "pnpm exec supabase gen types typescript --local > libs/common/src/lib/supabase.ts && pnpm exec prettier --write libs/common/src/lib/supabase.ts",
    "export:knowii": "pnpm exec nx export knowii",
    "format:check": "pnpm exec nx format:check",
    "format": "pnpm exec nx format:write",
    "lint": "pnpm exec next lint ./apps/knowii && pnpm run tsc",
    "migrate": "pnpm exec nx migrate latest",
    "postinstall": "pnpm run db:setup",
    "prepare": "pnpm exec husky install",
    "serve": "pnpm run start:knowii",
    "start": "pnpm run start:knowii",
    "start:knowii": "pnpm exec nx run knowii:serve",
    "storybook:client-ui": "cross-env TAILWIND_MODE=watch pnpm exec nx storybook client-ui",
    "tailwind": "tailwind --config ./apps/knowii/tailwind.config.js",
    "test": "pnpm run test:all",
    "test:all": "pnpm run test:client && pnpm run test:client-ui && pnpm run test:common && pnpm run test:knowii && pnpm run test:server",
    "test:client": "pnpm exec nx test client",
    "test:client-ui": "pnpm exec nx test client-ui",
    "test:common": "pnpm exec nx test common",
    "test:knowii": "pnpm exec nx test knowii",
    "test:server": "pnpm exec nx test server",
    "tsc": "pnpm exec tsc -p ./tsconfig.app.json --noEmit",
    "update": "pnpm run migrate"
  },
