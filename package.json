  "scripts": {
    "build": "npm run build:knowii",
    "build:knowii": "npx cross-env NODE_ENV=production npx nx run knowii:build",
    "clean": "npx rimraf ./dist",
    "clean:install": "npx rimraf package-lock.json && npx rimraf ./node_modules && npx rimraf dist && npm install",
    "commit": "git-cz",
    "cm": "npm run commit",
    "commit:lint": "commitlint --env HUSKY_GIT_PARAMS",
    "db:admin": "docker run --rm -p 5050:5050 thajeztah/pgadmin4",
    "db:clean": "npx rimraf ./supabase && npx supabase init && npx dotenv -e apps/knowii/.env.local -- ./tools/supabase-set-local-ports.sh && npx dotenv -e apps/knowii/.env.local -- ./tools/supabase-set-auth-providers.sh && npx dotenv -e apps/knowii/.env.local -- ./tools/supabase-enable-logging.sh",
    "db:client:generate": "npx prisma generate && npm run format",
    "db:init": "npm run db:migration:deploy && npm run db:seed",
    "db:logs:list": "docker exec -it supabase_db_knowii ls -la /var/log/postgresql",
    "db:logs:ls": "npm run db:logs:list",
    "db:logs:cat": "docker exec -it supabase_db_knowii cat",
    "db:logs:show": "docker exec -it supabase_db_knowii cat /var/log/postgresql/server_2000_UTC.csv",
    "db:migration:deploy": "dotenv -e apps/knowii/.env.local -- npx prisma migrate deploy --preview-feature",
    "db:migration:deploy:prod": "dotenv -e apps/knowii/.env.prod -- npx prisma migrate deploy --preview-feature",
    "db:migration:prepare": "dotenv -e apps/knowii/.env.local -- npx prisma migrate dev --preview-feature",
    "db:migration:status": "dotenv -e apps/knowii/.env.local -- npx prisma migrate status --preview-feature",
    "db:migration:status:prod": "dotenv -e apps/knowii/.env.prod -- npx prisma migrate status --preview-feature",
    "db:reset": "npm run db:stop && npm run db:clean && npm run db:start && npm run db:init && npm run db:types:generate && npm run db:client:generate",
    "db:schema:introspect": "dotenv -e apps/knowii/.env.local -- npx prisma introspect",
    "db:schema:introspect:prod": "dotenv -e apps/knowii/.env.prod -- npx prisma introspect",
    "db:schema:validate": "dotenv -e apps/knowii/.env.local -- npx prisma validate",
    "db:schema:validate:prod": "dotenv -e apps/knowii/.env.prod -- npx prisma validate",
    "db:setup": "npm run db:reset",
    "db:pull": "dotenv -e apps/knowii/.env.local -- npx prisma db pull",
    "predb:seed": "psql postgresql://postgres:postgres@localhost:12346/postgres -f supabase-db-seed.sql",
    "psql:shell": "psql postgresql://postgres:postgres@localhost:12346/postgres",
    "db:seed": "dotenv -e apps/knowii/.env.local -- npx prisma db seed",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:studio:prisma": "dotenv -e apps/knowii/.env.local -- npx prisma studio",
    "db:studio": "open http://localhost:12347",
    "db:generate:types": "npm run db:types:generate",
    "db:types:generate": "npx supabase gen types typescript --local > libs/common/src/lib/supabase.ts && npx prettier --write libs/common/src/lib/supabase.ts",
    "format:check": "npx nx format:check",
    "format": "npx nx format:write",
    "generate:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "generate:changelog-recent": "conventional-changelog -p angular | tail -n +3",
    "lint": "npm run tsc",
    "migrate": "npx nx migrate latest",
    "prepare": "npx husky install",
    "release": "release-it",
    "release:final": "release",
    "release:alpha": "release-it --preRelease=alpha",
    "release:beta": "release-it --preRelease=beta",
    "release:rc": "release-it --preRelease=rc",
    "serve": "npm run start:knowii",
    "start": "npm run start:knowii",
    "start:knowii": "NODE_OPTIONS=--max-old-space-size=4048 npx nx run knowii:serve",
    "start:prod": "npm run start:knowii:prod",
    "start:knowii:prod": "cd dist/apps/knowii; NODE_OPTIONS=--max-old-space-size=4048; dotenv -e ../../../apps/knowii/.env.local -- npx next start",
    "storybook:client-ui": "npx cross-env TAILWIND_MODE=watch npx nx storybook client-ui",
    "tailwind": "tailwind --config ./apps/knowii/tailwind.config.ts",
    "test": "npm run test:all",
    "test:all": "npm run test:client && npm run test:client-ui && npm run test:common && npm run test:knowii && npm run test:server",
    "test:client": "npx nx test client",
    "test:client-ui": "npx nx test client-ui",
    "test:common": "npx nx test common",
    "test:knowii": "npx nx test knowii",
    "test:server": "npx nx test server",
    "tsc": "npx tsc -p ./tsconfig.app.json --noEmit",
    "update": "npm run migrate"
  },
