{
  "name": "knowii",
  "version": "0.0.0",
  "license": "Unlicensed",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production npx nx run knowii:build",
    "clean": "npx rimraf ./dist",
    "clean:install": "npx rimraf ./package-lock.json && npx rimraf ./node_modules && npm install",
    "db:admin": "docker run --rm -p 5050:5050 thajeztah/pgadmin4",
    "db:backup": "mkdir -p ./backups/database && docker exec -u knowii knowii-db pg_dump -Cc | xz > ./backups/database/$(date -u +%Y-%m-%d)_postgres-knowii_backup.sql.xz",
    "db:clean": "docker-compose --file ./tools/docker/docker-compose-postgresql.yml rm --force --stop -v",
    "db:client:generate": "npx prisma generate",
    "db:migration:deploy": "dotenv -e apps/knowii/.env.local -- npx prisma migrate deploy --preview-feature",
    "db:migration:deploy:prod": "dotenv -e apps/knowii/.env.prod -- npx prisma migrate deploy --preview-feature",
    "db:migration:prepare": "dotenv -e apps/knowii/.env.local -- npx prisma migrate dev --preview-feature",
    "db:migration:status": "dotenv -e apps/knowii/.env.local -- npx prisma migrate status --preview-feature",
    "db:migration:status:prod": "dotenv -e apps/knowii/.env.prod -- npx prisma migrate status --preview-feature",
    "db:reset": "npm run db:clean && npm run db:start && npm run db:migration:deploy && npm run db:seed",
    "db:schema:introspect": "dotenv -e apps/knowii/.env.local -- npx prisma introspect",
    "db:schema:introspect:prod": "dotenv -e apps/knowii/.env.prod -- npx prisma introspect",
    "db:schema:validate": "dotenv -e apps/knowii/.env.local -- npx prisma validate",
    "db:schema:validate:prod": "dotenv -e apps/knowii/.env.prod -- npx prisma validate",
    "db:start": "docker-compose --file ./tools/docker/docker-compose-postgresql.yml up --detach && sleep 1s",
    "db:stop": "docker-compose --file ./tools/docker/docker-compose-postgresql.yml down",
    "db:studio": "dotenv -e apps/knowii/.env.local -- npx prisma studio",
    "export:knowii": "npx nx export knowii",
    "format:check": "npx nx format:check",
    "format": "npx nx format:write",
    "lint": "npx next lint ./apps/knowii && npx nx workspace-lint  && npm run tsc",
    "migrate": "npx nx migrate latest",
    "nx": "nx",
    "postinstall": "npm run db:client:generate",
    "prepare": "husky install",
    "serve": "npm run start:knowii",
    "start": "npm run start:knowii",
    "start:knowii": "npx nx run knowii:serve",
    "test": "npx nx test knowii",
    "tsc": "tsc -p ./tsconfig.app.json --noEmit",
    "update": "npm run migrate"
  },
  "dependencies": {
    "@nrwl/next": "15.6.3",
    "@prisma/client": "4.10.1",
    "@tailwindcss/typography": "0.5.9",
    "ajv": "8.12.0",
    "cuid": "3.0.0",
    "i18next": "22.4.9",
    "next": "13.1.1",
    "next-connect": "0.13.0",
    "next-themes": "0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.7.1",
    "rxjs": "7.8.0",
    "tailwindcss": "3.2.6",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@nrwl/cypress": "15.6.3",
    "@nrwl/eslint-plugin-nx": "15.6.3",
    "@nrwl/jest": "15.6.3",
    "@nrwl/js": "15.6.3",
    "@nrwl/linter": "15.6.3",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "15.6.3",
    "@nrwl/workspace": "15.6.3",
    "@testing-library/react": "13.4.0",
    "@types/jest": "28.1.1",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "autoprefixer": "10.4.13",
    "babel-jest": "28.1.1",
    "browserslist": "4.21.5",
    "concurrently": "7.6.0",
    "cross-env": "7.0.3",
    "cypress": "^12.2.0",
    "dotenv": "16.0.3",
    "dotenv-cli": "7.0.0",
    "eslint": "~8.15.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "lint-staged": "13.1.1",
    "nx": "15.6.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-import": "15.1.0",
    "postcss-preset-env": "8.0.1",
    "prettier": "^2.6.2",
    "prisma": "4.10.1",
    "react-test-renderer": "18.2.0",
    "rimraf": "4.1.2",
    "sass": "1.55.0",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  }
}
